# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

[js,ts]
indent_style = space
indent_size = 4

[webpack.config.js]
indent_style = space
indent_size = 2

[package.json]
indent_style = space
indent_size = 2

[tsconfig.json]
indent_style = space
indent_size = 2

[json]
indent_style = space
indent_size = 4

[scss]
indent_style = space
indent_size = 4
